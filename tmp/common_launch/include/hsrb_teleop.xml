<?xml version="1.0"?>
<launch>
  <arg name="namespace" default="/hsrb"/>
  <arg name="agent_num"/>
  <remap from="$(arg namespace)/arm_trajectory_controller/command" to="agent$(arg agent_num)/$(arg namespace)/arm_trajectory_controller/collisionable_command" />
  <remap from="$(arg namespace)/head_trajectory_controller/command" to="agent$(arg agent_num)/$(arg namespace)/head_trajectory_controller/collisionable_command" />
  <include file="$(find tmp)/launch/hsrb_interactive_teleop.launch">
  <arg name="agent_num" value="$(arg agent_num)"/>
  </include>
  <!--<group ns="$(arg namespace)">-->
  <group ns="agent$(arg agent_num)">
    <node pkg="joy" type="joy_node" name="joy_node">
      <param name="dev" value="/dev/input/js0" />
      <param name="deadzone" value="0.1" />
      <param name="autorepeat_rate" value="10" />
    </node>
  </group>
</launch>
